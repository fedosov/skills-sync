{
  "binary": "skillssync",
  "commands": [
    {
      "name": "sync",
      "args": ["--scope", "--json"],
      "example": "skillssync sync --scope all --json"
    },
    {
      "name": "watch",
      "args": ["--scope", "--interval-seconds"],
      "example": "skillssync watch --scope all --interval-seconds 5"
    },
    {
      "name": "skills install",
      "args": ["--scope"],
      "example": "skillssync skills install --scope all"
    },
    {
      "name": "skills list",
      "args": ["--scope", "--json"],
      "example": "skillssync skills list --scope project --json"
    },
    {
      "name": "skills add",
      "args": ["<package>", "--scope"],
      "example": "skillssync skills add owner/repo --scope project"
    },
    {
      "name": "skills remove",
      "args": ["<package>", "--scope"],
      "example": "skillssync skills remove owner/repo --scope project"
    },
    {
      "name": "skills update",
      "args": ["[package]", "--scope"],
      "example": "skillssync skills update owner/repo --scope all"
    },
    {
      "name": "mcp list",
      "args": ["--scope", "--json"],
      "example": "skillssync mcp list --scope all --json"
    },
    {
      "name": "mcp add",
      "args": ["<args...>", "--scope"],
      "example": "skillssync mcp add exa --scope project"
    },
    {
      "name": "mcp remove",
      "args": ["<args...>", "--scope"],
      "example": "skillssync mcp remove exa --scope project"
    },
    {
      "name": "migrate-dotagents",
      "args": ["--scope"],
      "example": "skillssync migrate-dotagents --scope all"
    },
    {
      "name": "doctor",
      "args": [],
      "example": "skillssync doctor"
    },
    {
      "name": "legacy commands",
      "args": [],
      "example": "delete/archive/restore/make-global/rename and mcp set-enabled are removed in strict mode"
    }
  ]
}
